            <string>/// Create me :) 
unstairs = false 

//Adjust room size 
room_width = (CELL_WIDTH/16)*1200
room_height = (CELL_HEIGHT/16)*1200

//Set grid width and height 
width = room_width div CELL_WIDTH; 
height = room_height div CELL_HEIGHT; 



///Create a giant Grid 
grid= ds_grid_create(width, height) 


/// Fill With Void.
ds_grid_set_region(grid,0,0,width-1, height-1,VOID);
//scr_new_room(10,10,10,10);
//scr_new_room(20,20,20,20);
scr_start_room (5,5,5,5);
scr_start_room (35,35,35,35);
//Randomize 
randomize(); 

//Create Controller Center of Grid 
cx = width div 2; 
cy = height div 2;


scr_random_path(); 
 


// may need optimized....but Lets add some walls 
for (var yy = 0; yy &lt; height; yy++){
      for (var xx =0; xx &lt;width; xx++){           

            /// Loop and make walls! 
            if (grid[# xx, yy] == FLOOR){
                if (grid[# xx+1, yy] != FLOOR)  grid[# xx+1,yy] = WALL;
                if (grid[# xx-1, yy] != FLOOR)  grid[# xx-1,yy] = WALL;
                if (grid[# xx, yy+1] != FLOOR)  grid[# xx,yy+1] = WALL;
                if (grid[# xx, yy-1] != FLOOR)  grid[# xx,yy-1] = WALL;
            }         
                  
            if (grid[# xx, yy] == HOUSE_FLOOR){
                if (grid[# xx+1, yy] != HOUSE_FLOOR)  grid[# xx+1,yy] = HOUSE_WALL;
                if (grid[# xx-1, yy] != HOUSE_FLOOR)  grid[# xx-1,yy] = HOUSE_WALL;
                if (grid[# xx, yy+1] != HOUSE_FLOOR)  grid[# xx,yy+1] = HOUSE_WALL;
                if (grid[# xx, yy-1] != HOUSE_FLOOR)  grid[# xx,yy-1] = HOUSE_WALL;
            }     
            
            
        }
   }
    
// draw level... Draw the grid 
for (var yy=0; yy &lt; height; yy++){
    for (var xx=0; xx &lt; width; xx++){
    
        if (grid[# xx, yy] == FLOOR)
            tile_add(bg_floor,0,0,CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT,0) 
        if (grid[# xx, yy] == WALL)
            tile_add(bg_wall,0,0,CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT,0) 
        if (grid[# xx, yy] == HOUSE_FLOOR)
            tile_add(bg_house_floor,0,0,CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT,0) 
        if (grid[# xx, yy] == HOUSE_WALL)
            tile_add(bg_house_wall,0,0,CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT,0)
    }
}
</string>
