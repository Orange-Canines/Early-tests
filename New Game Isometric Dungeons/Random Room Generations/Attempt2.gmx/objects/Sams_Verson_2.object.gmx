            <string>//Randomize 
randomize(); 
/// Create me :) 
unstairs = false 

//Adjust room size 
room_width = (CELL_WIDTH/16)*1200
room_height = (CELL_HEIGHT/16)*1200

//Set grid width and height 
width = room_width div CELL_WIDTH; 
height = room_height div CELL_HEIGHT; 


///Create a giant Grid 
grid= ds_grid_create(width, height) 


/// Fill With Void.
ds_grid_set_region(grid,0,0,width-1, height-1,VOID);


x1 = irandom_range(2, 40)
y1 = irandom_range(2, 40)
x2 = irandom_range(2, 20)
y2 = irandom_range(2, 20)
Lo1 = irandom_range(2, 20)
Lo2 = irandom_range(2, 20)

scr_start_room(x2,x2,x1,x1);
scr_end_room(x2,y2,y1,y1);
scr_path_to_room(y1,y1,x1,x1);
scr_path_loop(x1,x1,Lo1,Lo2);



//Create Controller Center of Grid 
cx = width div 2; 
cy = height div 2;


//scr_random_path(); 
 


// may need optimized....but Lets add some walls 
for (var yy = 0; yy &lt; height; yy++){
      for (var xx =0; xx &lt;width; xx++){           

            /// Loop and make walls! 
            if (grid[# xx, yy] == FLOOR){
                if (grid[# xx+1, yy] != FLOOR)  grid[# xx+1,yy] = WALL;
                if (grid[# xx-1, yy] != FLOOR)  grid[# xx-1,yy] = WALL;
                if (grid[# xx, yy+1] != FLOOR)  grid[# xx,yy+1] = WALL;
                if (grid[# xx, yy-1] != FLOOR)  grid[# xx,yy-1] = WALL;
            }         
                  
            if (grid[# xx, yy] == HOUSE_FLOOR){
                if (grid[# xx+1, yy] != HOUSE_FLOOR)  grid[# xx+1,yy] = HOUSE_WALL;
                if (grid[# xx-1, yy] != HOUSE_FLOOR)  grid[# xx-1,yy] = HOUSE_WALL;
                if (grid[# xx, yy+1] != HOUSE_FLOOR)  grid[# xx,yy+1] = HOUSE_WALL;
                if (grid[# xx, yy-1] != HOUSE_FLOOR)  grid[# xx,yy-1] = HOUSE_WALL;
            }     
            
            
        }
   }

scr_draw_tiles();
</string>
