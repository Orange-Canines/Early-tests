          <argument>
            <kind>1</kind>
            <string>/// Alpha change / counter shake

alphavar = alphavar - .01;
if instance_exists(obj_hero){
    x = obj_hero.x
    y = obj_hero.y-15
    
    if alphavar &gt; 0 {  
        if collected = 1{
            alphavar = 1;
            collected = 0;
            totalPoints += points;
        }    
        
        if global.heroState != 3 &amp;&amp; obj_hero.tmp != 3{
            orig_x = obj_hero.x-15;
            orig_y = obj_hero.y-35;
        }
        else{
            orig_x = obj_hero.x+15;
            orig_y = obj_hero.y+35;
        }
        for(i = .5; i &lt; 5; i+=.5){
            if(totalPoints &gt;= 100*i){
                if global.heroState != 3 &amp;&amp; obj_hero.tmp != 3{
                    scr_obj_shake(obj_hero.x-15,obj_hero.y-35,i/2);
                }
                else{
                    scr_obj_shake(obj_hero.x+15,obj_hero.y+35,i/2);
                }
            }
        }
    }
    else if (alphavar &lt;= 0){
        instance_destroy();
    } 
}
</string>
          </argument>
